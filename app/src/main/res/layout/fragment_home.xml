<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grismv"
    tools:context=".ui.home.HomeFragment">
    <RelativeLayout
        android:id="@+id/relativeLayoutHomeServ"

        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/ImageV_fondolog"
            android:layout_width="match_parent"
            android:layout_height="159dp"
            android:scaleType="centerCrop"
            android:src="@drawable/frac" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="110dp">

            <TextView
                android:id="@+id/textViewNumCasa"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="48dp"
                android:text="Casa 20"
                android:textColor="@color/purple_700"
                android:textSize="16sp"
                app:layout_constraintEnd_toStartOf="@+id/imageView_mas"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textViewNomFracc"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="FRACCIONAMIENTO"
                android:textAllCaps="false"
                android:textColor="@color/purple_700"
                android:textSize="16sp"
                app:layout_constraintEnd_toStartOf="@+id/imageView_mas"
                app:layout_constraintStart_toStartOf="@+id/textViewNumCasa"
                app:layout_constraintTop_toBottomOf="@+id/textViewNumCasa" />

            <ImageView
                android:id="@+id/imageView_mas"
                android:layout_width="81dp"
                android:layout_height="90dp"
                android:layout_marginEnd="16dp"
                android:src="@drawable/manitacent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="220dp">

            <ImageView
                android:id="@+id/imageViewNoticias"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16dp"
                android:src="@drawable/ic_ntc"
                app:layout_constraintEnd_toStartOf="@+id/imageViewAgenda"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintHorizontal_chainStyle="spread"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewAgenda"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16dp"
                android:src="@drawable/ic_agn"
                app:layout_constraintEnd_toStartOf="@+id/imageViewCatalogo"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewNoticias"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewCatalogo"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16dp"
                android:src="@drawable/servicios_ic"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewAgenda"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewEncuestas"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="132dp"
                android:src="@drawable/ic_enc"
                app:layout_constraintEnd_toStartOf="@+id/imageViewTransparenciah"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewTransparenciah"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="132dp"
                android:src="@drawable/ic_trn"
                app:layout_constraintEnd_toStartOf="@+id/imageViewDocumentosh"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewEncuestas"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewDocumentosh"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="132dp"
                android:src="@drawable/ic_dcm"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewTransparenciah"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clFragmentHomeLast"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="455dp"
            android:background="@color/white"
            android:paddingBottom="100dp">

            <ImageView
                android:id="@+id/imageViewPagosh"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="8dp"
                android:src="@drawable/ic_pgs"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageViewAdeudosh"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewAdeudosh"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="8dp"
                android:src="@drawable/ic_add"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageViewReciclaje"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewPagosh"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewReciclaje"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="8dp"
                android:src="@drawable/homereciclaje"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewAdeudosh"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintMenuPop"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="70dp"
            android:background="@color/common_google_signin_btn_text_light_focused"
            android:visibility="gone">

            <ImageView
                android:id="@+id/imageViewBuzon"
                android:layout_width="85dp"
                android:layout_height="100dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/manitaacceso"
                app:layout_constraintBottom_toTopOf="@+id/imageViewCerrarPop"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"></ImageView>

            <ImageView
                android:id="@+id/imageViewServicios"
                android:layout_width="85dp"
                android:layout_height="100dp"
                android:layout_marginTop="110dp"
                android:src="@drawable/manitaserv"
                app:layout_constraintEnd_toStartOf="@+id/imageViewBuzon"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageViewCerrarPop"
                android:layout_width="85dp"
                android:layout_height="100dp"
                android:src="@drawable/manitacent"
                app:layout_constraintBottom_toTopOf="@+id/imageViewReserva"
                app:layout_constraintEnd_toStartOf="@+id/imageViewAnuncios"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewServicios"
                app:layout_constraintTop_toBottomOf="@+id/imageViewBuzon" />

            <ImageView
                android:id="@+id/imageViewReserva"
                android:layout_width="85dp"
                android:layout_height="100dp"
                android:layout_marginBottom="8dp"
                android:src="@drawable/manitaa"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/imageViewCerrarPop"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/imageViewCerrarPop"
                app:layout_constraintTop_toBottomOf="@+id/imageViewCerrarPop" />

            <ImageView
                android:id="@+id/imageViewAnuncios"
                android:layout_width="85dp"
                android:layout_height="100dp"
                android:layout_marginTop="110dp"
                android:src="@drawable/manitab"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageViewBuzon"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.fragment.app.FragmentContainerView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        </androidx.fragment.app.FragmentContainerView>
    </RelativeLayout>
</ScrollView>
